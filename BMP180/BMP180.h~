#ifndef TMP102_h
#define TMP102_h

#include "Arduino.h"
#include "Wire.h"

class TMP102 {
	public:
		void begin();
		void read();
		void readPressure();
		void readTemp();
		double getTemp();
		double getPressure();
	private:
		int readInt(char address);
		unsigned int readUInt(char address);
		double _temperature;

};

#endif
